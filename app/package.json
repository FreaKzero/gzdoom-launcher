{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "./electron/main.js",
  "scripts": {
    "start": "concurrently \"yarn dev:webp\" \"yarn dev:elec\"",
    "dev:webp": "webpack-dev-server --mode development",
    "dev:elec": "electron .",
    "lint": "eslint -f=pretty 'src/**/*.js?(x)' --fix"
  },
  "babel": {
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "ssr": false
        }
      ]
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "author": "me",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "concurrently": "^5.0.0",
    "electron": "^7.0.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "prettier": "^1.18.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "css-loader": "^3.2.0",
    "electron-reload": "^1.5.0",
    "error-overlay-webpack-plugin": "^0.4.1",
    "file-loader": "^4.2.0",
    "i18next": "^19.0.0",
    "klaw": "^3.0.0",
    "klaw-sync": "^6.0.0",
    "nedb": "^1.8.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-i18next": "^11.0.1",
    "react-svg-inline": "^2.1.1",
    "style-loader": "^1.0.0",
    "styled-components": "^4.4.0",
    "svg-inline-loader": "^0.8.0",
    "uuid-quick": "^0.1.0",
    "wouter": "^2.3.1"
  }
}
